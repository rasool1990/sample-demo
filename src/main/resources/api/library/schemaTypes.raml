              depDateBirthH: string
              depEnglishFirstName: string
              depEnglishLastName: string
              depEnglishSecondName: string
              depEnglishThirdName: string
              depFamilyName: string
              depFatherName: string
              depFirstName: string
              depGender: string
              depGndFatherName: string
              depIqamaNo: string
              depNationalityNICCode: string
              depRelationship: string

