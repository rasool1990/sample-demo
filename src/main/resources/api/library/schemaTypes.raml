            type: integer
          firstName:
            type: string
            maxLength: 20
          secondName:
            type: string
            maxLength: 20
          thirdName:
            type: string
            maxLength: 20
          lastName:
            type: string
            maxLength: 20
          dateOfBirthH:
            type: string
            maxLength: 10
          gender:
