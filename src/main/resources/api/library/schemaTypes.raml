#%RAML 1.0 Library
usage: this library contains all the input and output schema types for all the resources invoked in API.
types:
  addressInfo:
    type: object
    properties:
      additionalNumber:
        type: integer
      buildingNumber:
        type: integer
      city:
        type: string
      district:
        type: string
      isPrimaryAddress:
        type: boolean
      locationCoordinates:
        type: string
      postCode:
        type: integer
      streetName:
        type: string
      unitNumber:
        type: integer

  userInfo:
    type: object
    properties:
      userName:
        type: string
        maxLength: 20
      password:
        type: string
        maxLength: 20
      chargeCode:
        type: string
        maxLength: 20
  
  englishNamesInfo:
    type: object
    properties:
      englishFirstName:
        type: string
        maxLength: 20
      englishSecondName:
        type: string
        maxLength: 20
      englishThirdName:
        type: string
        maxLength: 20
      englishLastName:
        type: string
        maxLength: 20
  
  alienStatuscommonTypes:
    type: userInfo
    properties:
      iqamaNumber:
        type: string
        maxLength: 10
      sponsorId:
        type: string
        maxLength: 10
      referenceNumber:
        type: string
        maxLength: 25

  alienStatusRequestType:
    description: payload structure for alien status request
    type: alienStatuscommonTypes
    
  alienStatusResponseType:
    description: payload structure for alien status response
    type: object
    properties:
              logId:
                type: integer
              legalStatus:
                type: boolean

  alienInfoByIqamaRequestType:
    description: payload structure for alien info request
    type: alienStatuscommonTypes
    

  alienInfoByIqamaResponseType:
    description: payload structure for alien info response
    type: englishNamesInfo
    properties:
          logId:
            type: integer
          firstName:
            type: string
            maxLength: 20
          secondName:
            type: string
            maxLength: 20
          thirdName:
            type: string
            maxLength: 20
          lastName:
            type: string
            maxLength: 20
          dateOfBirthG:
            type: string
            maxLength: 10
          gender:
            type: string
            enum: [M, F, U]
            description: "M:Male, F:Female, U:Undefined"
          sponsorName:
            type: string
            maxLength: 20
          socialStatusCode:
            type: string
            maxLength: 5
          nationalityCode:
            type: string
            maxLength: 5
          occupationCode:
            type: integer
          iqamaExpiryDateG:
            type: string

  alienInfoByBorderNumberRequestType:
    type: userInfo
    properties:
      borderNumber:
            type: string
            maxLength: 10
      sponsorId:
            type: string
            maxLength: 10
      referenceNumber:
            type: string
            maxLength: 25
  
  alienInfoByBorderNumberResponseType:
    type: object
    properties:
      logId :
        type: integer
      firstName:
        type: string
        maxLength: 20
      seconedName:
        type: string
        maxLength: 20
      grandFatherName:
        type: string
        maxLength: 20
      familyName:
        type: string
        maxLength: 20
      gender:
        type: string
        enum: [M, F, U]
        description: "M:Male, F:Female, U:Undefined"
      sponsorName:
        type: string
        maxLength: 20
      dateOfBirthG:
        type: string
        maxLength: 10
        description: "The value should be in this format  (dd-MM-yyyy)"
      nationalityCode:
        type: string
        maxLength: 5
      occupationCode:
        type: integer
    

  alienInfoByIqamaAndDependentRequestType:
    type: userInfo
